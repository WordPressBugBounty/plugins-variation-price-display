{
  "name": "variaton-price-display",
  "version": "1.3.21",
  "description": "Adds lots of advanced options to control how you display the price for your WooCommerce variable products.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "min-js": "minify  public/js/public.js > public/js/public.min.js",
    "min-css": "minify  public/css/public.css > public/css/public.min.css",
    "rtl-css": "rtlcss  public/css/public.css public/css/public-rtl.css",
    "rtl-min-css": "minify  public/css/public-rtl.css > public/css/public.min-rtl.css",
    "wpx-min-css": "minify  includes/wpxtension/wpxtension-admin.css > includes/wpxtension/wpxtension-admin.min.css",
    "wpx-rtl-css": "rtlcss  includes/wpxtension/wpxtension-admin.css includes/wpxtension/wpxtension-admin-rtl.css",
    "wpx-rtl-min-css": "minify  includes/wpxtension/wpxtension-admin-rtl.css > includes/wpxtension/wpxtension-admin.min-rtl.css",
    "admin-min-js": "minify  admin/js/backend.js > admin/js/backend.min.js",
    "admin-min-css": "minify  admin/css/backend.css > admin/css/backend.min.css",
    "all": "npm-run-all --serial min-js min-css rtl-css rtl-min-css wpx-min-css wpx-rtl-css wpx-rtl-min-css admin-min-js admin-min-css"
  },
  "author": "wpxteam",
  "license": "ISC",
  "dependencies": {
    "minify": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "rtlcss": "^4.3.0"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": false,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  }
}
